{
  "name": "udes-cli",
  "version": "0.2.0",
  "description": "CLI tools used at UdeS",
  "dependencies": {
    "shelljs": "0.7.8",
    "uglify-es": "^3.1.5",
    "yargs": "10.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.1",
    "eslint-config-udes": "^0.1.6",
    "mocha": "^4.0.1",
    "nsp": "2.8.1",
    "nyc": "^11.2.1",
    "sinon": "^4.0.2",
    "sinon-chai": "^2.14.0"
  },
  "scripts": {
    "audit": "node_modules/.bin/nsp check",
    "build": "babel ./src --out-dir dist --ignore '**/*Test.js'",
    "coverage": "cross-env NODE_ENV=test node_modules/.bin/nyc --require babel-core/register -x '**/**Test.js' node_modules/.bin/mocha --opts ../mocha.opts \\ ./src/**/**Test.js",
    "format": "eslint . --ignore-path .gitignore --fix",
    "lint": "eslint . --ignore-path .gitignore",
    "prepublishOnly": "npm run test && npm run build",
    "test": "cross-env NODE_ENV=test mocha --require babel-core/register --opts ../mocha.opts ./src/**/**Test.js",
    "udes-lint": "udes lint",
    "udes-polymer-build": "udes polymer-build"
  },
  "bin": {
    "udes": "bin/udes.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UdeS-STI/udes-cli.git"
  },
  "keywords": [
    "cli",
    "utils",
    "script"
  ],
  "author": "JF Belisle <jfbelisle89@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UdeS-STI/udes-cli/issues"
  },
  "homepage": "https://github.com/UdeS-STI/udes-cli#readme"
}
