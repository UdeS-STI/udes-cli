{
  "name": "udes-cli",
  "version": "0.1.8",
  "description": "Utility fonctions to use in npm script",
  "dependencies": {
    "cpx": "^1.5.0",
    "inline-source-cli": "^1.2.0",
    "replace-in-file": "^2.5.3",
    "shelljs": "0.7.8",
    "yargs": "9.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "eslint-config-udes": "^0.1.5",
    "mocha": "^4.0.1",
    "nyc": "^11.2.1",
    "sinon": "^4.0.2",
    "sinon-chai": "^2.14.0"
  },
  "scripts": {
    "build": "babel ./src --out-dir dist",
    "coverage": "NODE_ENV=test node_modules/.bin/nyc --require babel-core/register -x '**/**Test.js' node_modules/.bin/mocha --opts ../mocha.opts \\ ./src/**/**Test.js",
    "lint": "eslint ./src; eslint ./bin",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --opts ../mocha.opts ./src/**/**Test.js",
    "udes-polymer-build": "polymer-build"
  },
  "bin": {
    "polymer-build": "bin/polymer/polymer-build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UdeS-STI/udes-cli.git"
  },
  "keywords": [
    "cli",
    "utils",
    "script"
  ],
  "author": "JF Belisle <jfbelisle89@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UdeS-STI/udes-cli/issues"
  },
  "homepage": "https://github.com/UdeS-STI/udes-cli#readme"
}
