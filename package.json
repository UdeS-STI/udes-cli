{
  "name": "udes-cli",
  "version": "0.6.2",
  "description": "CLI tools used at UdeS",
  "dependencies": {
    "@udes/bower-locker": "^1.0.6",
    "@udes/shelljs-nodecli": "^0.2.3",
    "udes-logger": "^0.2.0",
    "update-notifier": "^2.3.0",
    "yargs": "^11.0.0"
  },
  "peerDependencies": {
    "bower": "^1.8.2",
    "polymer-cli": "^1.5.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint-config-udes": "^0.2.1",
    "mocha": "^5.0.1",
    "npm-run-all": "^4.1.2",
    "nsp": "^3.2.1",
    "nyc": "^12.0.1",
    "sinon": "^5.0.0",
    "sinon-chai": "^3.0.0"
  },
  "scripts": {
    "audit": "nsp check",
    "build": "babel ./src --out-dir dist --ignore '**/*Test.js' --ignore '**/test/**'",
    "coverage": "cross-env NODE_ENV=test NOLOG=true node_modules/.bin/nyc --require babel-core/register -x '**/**Test.js' node_modules/.bin/mocha --opts ../mocha.opts \\ ./src/**/**Test.js",
    "format": "npm-run-all format:*",
    "format:js": "eslint . --ext js --fix",
    "format:json": "eslint . --ext json --fix",
    "format:markdown": "eslint . --ext md --fix",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint . --ext js",
    "lint:json": "eslint . --ext json",
    "lint:markdown": "eslint . --ext md",
    "prepublishOnly": "npm run test && npm run build",
    "test": "cross-env NODE_ENV=test NOLOG=true mocha --require babel-core/register --opts ../mocha.opts ./src/**/**Test.js"
  },
  "bin": {
    "udes": "bin/udes.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UdeS-STI/udes-cli.git"
  },
  "keywords": [
    "cli",
    "utils",
    "script"
  ],
  "author": "Universit√© de Sherbrooke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UdeS-STI/udes-cli/issues"
  },
  "homepage": "https://github.com/UdeS-STI/udes-cli#readme"
}
